{
  "name": "minervaarcher-vue",
  "version": "0.2.0",
  "description": "My personal website, rewritten using Vue.js",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/MinervaArcher/minervaarcher-vue.git"
  },
  "scripts": {
    "start": "webpack-dev-server --open --mode=development",
    "build": "node --trace-deprecation node_modules/.bin/webpack --mode=production; chmod -R 755 ./dist",
    "build-dev": "webpack --mode=development; chmod -R 755 ./dist",
    "test": "jest"
  },
  "author": "Minerva Archer",
  "license": "MIT",
  "dependencies": {
    "intersection-observer": "^0.11.0",
    "register-service-worker": "^1.7.1",
    "vue": "^2.6.11",
    "vue-router": "^3.3.4"
  },
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "@vue/babel-preset-app": "^4.4.6",
    "@vue/cli-plugin-pwa": "^4.4.6",
    "@vue/preload-webpack-plugin": "^1.1.1",
    "@vue/test-utils": "^1.0.3",
    "autoprefixer": "^9.8.5",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.1.0",
    "babel-preset-vue": "^2.0.2",
    "case-sensitive-paths-webpack-plugin": "^2.3.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.0.3",
    "css-loader": "^3.6.0",
    "eslint": "^7.4.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-vue": "^6.2.2",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^26.1.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.14.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "sass-loader": "^9.0.2",
    "style-loader": "^1.2.1",
    "terser-webpack-plugin": "^3.0.6",
    "url-loader": "^4.1.0",
    "vue-jest": "^4.0.1",
    "vue-loader": "^15.9.3",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "workbox-webpack-plugin": "^5.1.3"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "jest": {
    "verbose": true,
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/jestAssetsTransformer.js"
    },
    "transform": {
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest",
      "^.+\\.(js|jsx)?$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/"
    ]
  }
}
